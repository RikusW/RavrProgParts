# vim:ts=4 sts=0 sw=4

ADMIN
	PART_NAME=ATmega163
	CORE_VERSION=V2E
	SPEED=8MHZ
	SIGNATURE=$1E9402
	TOGGLE_PIN=N
	OSCCAL=1.0
	PROGIF=isp,hvpp

MEMORY
	FLASH=16384,128
	EEPROM=512,0
	IRAM=1024,$60
	ERAM=0,NA

BOOT
	RWW=NA,NA
	NRWW=0,$1FFF
	BOOTSIZES=$1F80,$1F00,$1E00,$1C00

INTERRUPTS
	$000=RESET
	$002=INT0
	$004=INT1
	$006=TIMER2_COMP
	$008=TIMER2_OVF
	$00A=TIMER1_CAPT
	$00C=TIMER1_COMPA
	$00E=TIMER1_COMPB
	$010=TIMER1_OVF
	$012=TIMER0_OVF
	$014=SPI,STC
	$016=UART,RX
	$018=UART,UDRE
	$01A=UART,TX
	$01C=ADC
	$01E=EE_RDY
	$020=ANA_COMP
	$022=TWI

IO_MODULE
