		LIST=[ADMUX:ADCSR:ADCH:ADCL]
	ANALOG_COMPARATOR
		LIST=[SFIOR:ACSR]
	BOOT_LOAD
		LIST=[SPMCR]
	CPU
		LIST=[SREG:SPH:SPL:OSCCAL:MCUCR:MCUSR:SFIOR]
		LINK=[SPH:SPL]
	EEPROM
		LIST=[EEARH:EEARL:EEDR:EECR]
		LINK=[EEARH:EEARL]
	EXTERNAL_INTERRUPT
		LIST=[GIMSK:GIFR]
	PORTA
		LIST=[PORTA:DDRA:PINA]
	PORTB
		LIST=[PORTB:DDRB:PINB]
	PORTC
		LIST=[PORTC:DDRC:PINC]
	PORTD
		LIST=[PORTD:DDRD:PIND]
	SPI
		LIST=[SPDR:SPSR:SPCR]
	TIMER_COUNTER_0
		LIST=[TIMSK:TIFR:TCCR0:TCNT0]
	TIMER_COUNTER_1
		LIST=[TIMSK:TIFR:TCCR1A:TCCR1B:TCNT1H:TCNT1L:OCR1AH:OCR1AL:OCR1BH:OCR1BL:ICR1H:ICR1L]
		LINK=[TCNT1H:TCNT1L];[OCR1AH:OCR1AL];[OCR1BH:OCR1BL];[ICR1H:ICR1L]
	TIMER_COUNTER_2
		LIST=[TIMSK:TIFR:TCCR2:TCNT2:OCR2:ASSR]
	TWI
		LIST=[TWBR:TWCR:TWSR:TWDR:TWAR]
	UART
		LIST=[UDR:UCSRA:UCSRB:UBRRHI:UBRR]
	WATCHDOG
		LIST=[WDTCR]

