STK500
	IspChipErase
		eraseDelay=33
		pollMethod=0
	IspProgramFlash
		mode=$11
		blockSize=64
		delay=20
		cmd1=$40
		cmd2=$4C
		cmd3=$20
		pollVal1=$FF
		pollVal2=$00
	IspProgramEeprom
		mode=$04
		blockSize=128
		delay=20
		cmd1=$C0
		cmd2=$00
		cmd3=$A0
		pollVal1=$FF
		pollVal2=$FF
	PPControlStack=0E1E0F1F2E3E2F3F4E5E4F5F6E7E6F7F667667776A7A6B7BBEFD000100000000
	PpEnterProgMode
		stabDelay=100
		progModeDelay=0
		latchCycles=0
		toggleVtg=0
		powerOffDelay=0
		resetDelayMs=0
		resetDelayUs=0
	PpChipErase
		pulseWidth=0
		pollTimeout=30
	PpProgramFlash
		pollTimeout=15
		mode=$0F
		blockSize=256
	PpProgramEeprom
		pollTimeout=5
		mode=$00
		blockSize=256
	PpProgramFuse
		pulseWidth=0
		pollTimeout=2
	PpProgramLock
		pulseWidth=0
		pollTimeout=2

STK600

FUSES
	HIGH
		BOOTSZ=$06,Select Boot Size
			$03=Boot Flash size=128 words start address=$1F80
			$02=Boot Flash size=256 words start address=$1F00
			$01=Boot Flash size=512 words start address=$1E00
			$00=Boot Flash size=1024 words start address=$1C00
		BOOTRST=$01,Boot Reset vector Enabled
	LOW=$D2
		BODLEVEL=$80,Brown out detector trigger level
			$00=Brown-out detection at VCC=4.0 V
			$01=Brown-out detection at VCC=2.7 V
		BODEN=$40,Brown-out detection enabled
		SPIEN=$20,Serial program downloading (SPI) enabled
		CKSEL=$0F,Select Clock Source
			$00=External Clock fast rising power
			$01=External Clock BOD enabled
			$02=Internal RC Ocsillator slowly rising power ; default value
			$03=Internal RC Ocsillator fast rising power
			$04=Internal RC Oscillator BOD enabled
			$05=External RC Oscillator slowly rising power
			$06=External RC Oscillator fast rising power
			$07=External RC Oscillator BOD enabled
			$08=External Low-Frequency Crystal
			$09=External Low-Frequency Crystal
			$0A=Crystal Oscillator slowly rising power
			$0B=Crystal Oscillator fast rising power
			$0C=Crystal Oscillator BOD enabled
			$0D=Ceramic Resonator/External Clock slowly rising power
			$0E=Ceramic Resonator fast rising power
			$0F=Ceramic Resonator BOD enabled

LOCKBITS
	LB=$03,Memory Lock
		$00=Further programming and verification disabled
		$02=Further programming disabled
		$03=No memory lock features enabled
	BLB0=$0C,Boot Loader Protection Mode
		$00=LPM and SPM prohibited in Application Section
		$01=LPM prohibited in Application Section
		$02=SPM prohibited in Application Section
		$03=No lock on SPM and LPM in Application Section
	BLB1=$30,Boot Loader Protection Mode
		$00=LPM and SPM prohibited in Boot Section
		$01=LPM prohibited in Boot Section
		$02=SPM prohibited in Boot Section
		$03=No lock on SPM and LPM in Boot Section


