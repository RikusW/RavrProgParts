# vim:ts=4 sts=0 sw=4

ADMIN
	PART_NAME=ATtiny40
	CORE_VERSION=AVR8L_0
	SPEED=12MHZ
	SIGNATURE=$1E920E
	TOGGLE_PIN=Y
	PROGIF=TPI
	OSCCAL=8.0

MEMORY
	FLASH=4096,128
	EEPROM=0,0
	IRAM=256,$40

INTERRUPTS
	$000=RESET
	$001=INT0
	$002=PCINT0
	$003=PCINT1
	$004=WDT
	$005=TIM1_CAPT
	$006=TIM1_COMPA
	$007=TIM1_COMPB
	$008=TIM1_OVF
	$009=TIM0_COMPA
	$00A=TIM0_COMPB
	$00B=TIM0_OVF
	$00C=ANA_COMP
	$00D=ADC ADC
	$00E=TWI_SLAVE
	$00F=SPI
	$010=QTRIP

IO_MODULE
	AD_CONVERTER
		LIST=[ADMUX:ADCSRA:ADCH:ADCL:ADCSRB:DIDR0]
	ANALOG_COMPARATOR
		LIST=[ACSRA:ACSRB]
	CPU
		LIST=[CCP:SPH:SPL:SREG:CLKMSR:CLKPSR:OSCCAL:PRR:RSTFLR:NVMCSR:NVMCMD:MCUCR:GIMSK:GIFR:RAMAR:RAMDR]
