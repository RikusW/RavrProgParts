FUSES
	LOW
		BODLEVEL=$70,Brown-out Detector trigger level
			$07=Brown-out detection disabled; [BODLEVEL=111]
			$06=Brown-out detection at VCC=1.8 V
			$05=Brown-out detection at VCC=2.7 V
			$04=Brown-out detection at VCC=4.3 V
		CKOUT=$04,Output external clock
		WDTON=$02,Watch dog timer always on
		RSTDISBL=$01,Disable external reset

LOCKBITS
	LB=$03,Memory Lock
