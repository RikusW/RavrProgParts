	EXTERNAL_INTERRUPT
		LIST=[PCMSK2:PCMSK1:PCMSK0]
	PORTA
		LIST=[PORTCR:PUEA:PORTA:DDRA:PINA]
	PORTB
		LIST=[PORTCR:PUEB:PORTB:DDRB:PINB]
	PORTC
		LIST=[PORTCR:PUEC:PORTC:DDRC:PINC]
	SPI
		LIST=[SPDR:SPSR:SPCR]
	TIMER_COUNTER_0
		LIST=[TCCR0A:TCCR0B:TCCR1A:TCNT1H:TCNT1L:OCR1A:OCR1B:TIMSK:TIFR:TCNT0:OCR0A:OCR0B]
		LINK=[TCNT0H:TCNT0L]
	TWI
		LIST=[TWSCRA:TWSCRB:TWSSRA:TWSA:TWSD:TWSAM]
	WATCHDOG
		LIST=[WDTCSR]

