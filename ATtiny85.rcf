# vim:ts=4 sts=0 sw=4

ADMIN
	PART_NAME=ATtiny85
	CORE_VERSION=V2a
	SPEED=20MHZ
	SIGNATURE=$1E930B
	TOGGLE_PIN=Y
	OSCCAL=8.0,6.4
	PROGIF=isp,hvsp,dw

MEMORY
	FLASH=8192,64
	EEPROM=512,4
	IRAM=512,$60
	ERAM=0,NA

BOOT
	RWW=$0,$0
	NRWW=$0,$FFF

INTERRUPTS
	$000=RESET
	$001=INT0
	$002=PCINT0
	$003=TIMER1_COMPA
	$004=TIMER1_OVF
	$005=TIMER0_OVF
	$006=EE_RDY
	$007=ANA_COMP
	$008=ADC
	$009=TIMER1_COMPB
	$00A=TIMER0_COMPA
	$00B=TIMER0_COMPB
	$00C=WDT
	$00D=USI_START
	$00E=USI_OVF

OCD
	REV=1
	OCDR=$22
	ID=$930B
	IF=DebugWire
	ucRead=F8E1FFF1FBFFBFEF
	ucWrite=C8E1FF71BB7FADEB
	ucExtRead=0000000000000000000000000000000000000000
	ucExtWrite=0000000000000000000000000000000000000000
	ucEepromInst=BB FF BB EE BB CC B2 0D BC 02 B4 02 BA 0D BB BC 99 E1 BB AC
	ucFlashInst=B4 02 12
	ucUpperExtIOLoc=$00

IO_MODULE
